# Frontend Environment Configuration Template
# Copy this file to .env and update with your values

# API Configuration
API_URL=http://localhost:5000/api
API_TIMEOUT=10000
MAX_RETRY_ATTEMPTS=3

# App Configuration
APP_NAME=StrongClone
ENV_TYPE=development

# Feature Flags
ENABLE_DEBUG_MODE=true
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false

# Security
ENABLE_BIOMETRIC_AUTH=true
ENABLE_SECURE_STORAGE=true

# App Configuration
DEFAULT_LANGUAGE=en
TIMEZONE=UTC

# Analytics Keys (Production only)
ANALYTICS_KEY=your_analytics_key_here
CRASH_REPORTING_KEY=your_crash_reporting_key_here

# Push Notifications (Future use)
PUSH_NOTIFICATION_SERVER_KEY=your_push_key_here

# Social Login (Future use)
GOOGLE_CLIENT_ID=your_google_client_id
APPLE_CLIENT_ID=your_apple_client_id
