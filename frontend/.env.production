# Fitera Production Environment Configuration
# Last Updated: January 2025
# WARNING: This file contains sensitive configuration - DO NOT commit to version control

# Application Configuration
APP_NAME=Fitera
APP_VERSION=1.0.0
BUILD_NUMBER=1
ENV_TYPE=production
BUNDLE_ID_IOS=com.fitera.app
BUNDLE_ID_ANDROID=com.fitera.app

# API Configuration
API_BASE_URL=https://api.fitera.com
API_VERSION=v1
API_TIMEOUT=30000
MAX_RETRY_ATTEMPTS=3
ENABLE_REQUEST_LOGGING=false

# Security Configuration
ENABLE_HTTPS_ENFORCEMENT=true
ENABLE_CERTIFICATE_PINNING=true
ENABLE_SECURITY_HEADERS=true
ENABLE_RATE_LIMITING=true
ENABLE_DEBUG_MODE=false
ENABLE_SECURE_STORAGE=true
ENABLE_BIOMETRIC_AUTH=true

# Database Configuration
DATABASE_NAME=fitera_prod.db
DATABASE_VERSION=1
ENABLE_DATABASE_ENCRYPTION=true
ENABLE_FOREIGN_KEYS=true
ENABLE_WAL_MODE=true
DATABASE_CACHE_SIZE=10000

# Logging Configuration
LOG_LEVEL=error
ENABLE_CRASH_REPORTING=true
ENABLE_ANALYTICS=true
ENABLE_SECURITY_AUDIT=true
SECURITY_LOG_RETENTION_DAYS=90
ERROR_LOG_RETENTION_DAYS=30

# Performance Configuration
ENABLE_CACHE=true
CACHE_SIZE_MB=50
IMAGE_CACHE_SIZE_MB=100
ENABLE_LAZY_LOADING=true
ENABLE_PERFORMANCE_MONITORING=true

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_CLOUD_SYNC=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_SOCIAL_FEATURES=false
ENABLE_PREMIUM_FEATURES=true

# Localization
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,es,fr,de
TIMEZONE=UTC

# Third Party Services
# Replace with actual production keys
SENTRY_DSN=https://your-production-key@sentry.io/project-id
ANALYTICS_KEY=your-production-analytics-key
CRASH_REPORTING_KEY=your-production-crash-key
PUSH_NOTIFICATION_KEY=your-production-push-key

# CDN Configuration
CDN_BASE_URL=https://cdn.fitera.com
ASSET_VERSION=1.0.0
ENABLE_CDN_FALLBACK=true

# Support Configuration
SUPPORT_EMAIL=support@fitera.com
PRIVACY_POLICY_URL=https://fitera.com/privacy
TERMS_OF_SERVICE_URL=https://fitera.com/terms
HELP_CENTER_URL=https://help.fitera.com

# Certificate Pinning (SHA-256)
CERT_PIN_PRIMARY=sha256/AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA=
CERT_PIN_BACKUP=sha256/BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBB=

# Rate Limiting Configuration
RATE_LIMIT_AUTH_REQUESTS=5
RATE_LIMIT_AUTH_WINDOW=900
RATE_LIMIT_API_REQUESTS=100
RATE_LIMIT_API_WINDOW=60

# Monitoring Endpoints
HEALTH_CHECK_URL=https://api.fitera.com/health
STATUS_PAGE_URL=https://status.fitera.com
